    <link rel="shortcut icon" href="~/upload/favicon_mjp_icon.ico">
    <link rel="apple-touch-icon" sizes="144x144" type="image/x-icon" href="/upload/favicon/apple-touch-icon.png">


    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
