                    </ul>
                    <!-- /social -->
                    @Html.Partial("_Navbar")
                </div>
            </div>
        </div>
        <!-- /NAV -->
    </header>
    <!-- /HEADER -->
   @RenderBody();
    <!-- /SECTION -->
   @Html.Partial("_Footer")
